@model IEnumerable<CHA.Data.Crewmem>

@{
    ViewData["Title"] = "Index";
}

<header>
    <div class="main">
        <div class="logo">
            <img src="~/images/airlineicondownload.png" />
        </div>
        <br />
        <br />
        <br />
        <br />

        <div class="title">
            <h1>CORNHUSKER AIRWAY-CHA</h1>
        </div>
        <br />
        <br />
        <br />
        <ul>
            <li><a href="/Home">Home</a></li>
            <li class="active"><a href="/Crewmems">Crew Schedule</a></li>
            <li><a href="/AboutUs">About Us</a></li>
            <li><a href="/ContactUs">Contact Us</a></li>
            <li><a href="/login">Login</a></li>
        </ul>
    </div>
</header>
<br />
<br />
<br />
<br />
<br />
<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.CrewID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.CrewID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.CrewID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
